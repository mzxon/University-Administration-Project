<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
     "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
     
<!-- hakjuk.xml -->

<mapper namespace="hakjuk">

	<!-- 학과 목록 -->
	<select id="hakgwalist" resultType="java.util.Map">
		SELECT hgcode, dcode, dname, hcode, hname, hgname
		FROM hakgwa
		ORDER BY hgcode
	</select>

</mapper>
